    "antd": "^4.18.2",
    "classnames": "^2.3.1",
    "codemirror": "^5.65.0",
    "color-string": "^1.9.0",
    "echarts": "^5.2.2",
    "lodash": "^4.17.21",
    "rc-color-picker": "^1.2.6",
    "react": "17.x",
    "react-beautiful-dnd": "^13.1.0",
    "react-codemirror2": "^7.2.1",
    "react-colorful": "^5.5.1",
    "react-dom": "17.x",
    "react-rnd": "^10.3.5",
    "umi": "^3.5.20",
    "umi-request": "^1.4.0"

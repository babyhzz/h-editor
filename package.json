    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
